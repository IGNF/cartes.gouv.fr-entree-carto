{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["./src/auto-imports.d.ts", "./env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*", "src/**/*.spec.ts"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
