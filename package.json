{
  "name": "cartes.gouv.fr-entree-carto",
  "type": "module",
  "version": "1.0.13",
  "description": "Interface cartographique permettant de visualiser et manipuler les données de la Géoplateforme",
  "date": "19/12/2025",
  "directories": {},
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IGNF/cartes.gouv.fr-entree-carto.git"
  },
  "author": "IGNF",
  "keywords": [
    "geoportail",
    "geoplateforme",
    "javascript",
    "openlayers",
    "vue3",
    "dsfr",
    "cartes.gouv.fr"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/IGNF/cartes.gouv.fr-entree-carto/issues"
  },
  "homepage": "https://github.com/IGNF/cartes.gouv.fr-entree-carto#readme",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build-only": "vite build",
    "dev": "vite --force --host --mode development-local",
    "dev-local": "vite --force --host --mode docker-local",
    "build": "run-p type-check \"build-only {@}\" --",
    "format": "eslint ./src --fix",
    "lint": "eslint ./src",
    "icons": "npx vue-dsfr-icons -s scripts/icons.js -t scripts/icons-collections.ts",
    "test:unit": "npx vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npm run test:e2e -- --ui --project=chromium",
    "test:e2e:debug": "npm run test:e2e -- --debug --workers=1",
    "test:e2e:trace": "npm run test:e2e -- --trace on",
    "test:e2e:report": "playwright show-report",
    "type-check": "vue-tsc --build --force",
    "preview": "vite preview",
    "serve": "BASE_URL=/ vite build && serve -s dist/",
    "ext:i": "rm -rf node_modules/geopf-extensions-openlayers package-lock.json && npm install --force"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.13.2",
    "@gouvminint/vue-dsfr": "^8.12.1",
    "@iconify/vue": "^4.3.0",
    "geopf-extensions-openlayers": "./geopf-extensions-openlayers-1.0.0-beta.7-477.tgz",
    "jspdf": "^3.0.1",
    "keycloak-js": "^26.2.2",
    "ol": "^10.5.0",
    "serve": "^14.2.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@badgateway/oauth2-client": "^2.4.2",
    "@iconify-json/ri": "^1.2.6",
    "@playwright/test": "^1.52.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/components": "^13.1.0",
    "@vueuse/core": "^13.1.0",
    "ajv-formats": "^2.1.1",
    "eslint": "^9.24.0",
    "eslint-plugin-vue": "^10.0.0",
    "express": "^5.1.0",
    "focus-trap": "^7.6.4",
    "focus-trap-vue": "^4.0.3",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "mitt": "^3.0.1",
    "msw": "2.10.4",
    "notivue": "^2.4.5",
    "npm-run-all2": "^7.0.2",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.1.7",
    "pinia-plugin-store": "^2.2.9",
    "rimraf": "^6.0.1",
    "sass": "^1.86.3",
    "typescript": "~5.8.3",
    "universal-base64url": "^1.1.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.2.6",
    "vite-plugin-env-runtime": "./modules/vite-plugin-env-runtime-0.3.6.tgz",
    "vitest": "^3.1.2",
    "vue": "^3.5.13",
    "vue-logger-plugin": "^2.2.3",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
